//temp data
var row = [{
	"title" : "Fish", "title" : "Banana", "title" : "Soup"
}];


//create list section
var listSec = Ti.UI.createListSection({
	
});

//create our list view
var list1 = Ti.UI.createListView({
	style : Ti.UI.iPhone.ListViewStyle.GROUPED,
	backgroundColor : "#C3E4ED",
	hasChild : true
});


//add our list items
listSec.setItems(row);
list1.appendSection(listSec);

//export our list
exports.list = list1;