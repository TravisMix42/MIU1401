var objs = require("objects");
var data = [];

for(var i = 0; i < objs.objects.length; i++){
	data[i] = Ti.UI.createPickerRow({title : objs.objects[i].name});
}

exports.work = data;

var label1 = Ti.UI.createLabel({
	top : 100,
	bottom : 0, 
	right : 0,
	left : 0,
	text : ""
});

while(true){
	label1.text = objs.objects[i].about();
}
