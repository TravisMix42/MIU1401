//temp data
var temp = Array("fish", "bannana", "apple");
var row;

//create list section
var list1Sec = Ti.UI.createListSection({
	for(var i = 0; i < temp.length; i++){
		row = temp[i];
		row.push();
	}
});

//create our list view
var list1 = Ti.UI.createListView({
	style : Ti.UI.iPhone.ListViewStyle.PLAIN,
	backgroundColor : "#C3E4ED",
	hasChild : true
});


//add our list items


//export our list
exports.list = list1;