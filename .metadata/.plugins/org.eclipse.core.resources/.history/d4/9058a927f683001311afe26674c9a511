//create our window
var detWin = Ti.UI.createWindow({
	title : games[i].name
});

var detTitleLbl = Ti.UI.createLabel({
	title : "Name: " + games[i].name,
	top : 20,
	left : 0,
	right : 0
});

detWin.add(detTitleLbl);
