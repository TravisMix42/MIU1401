var r;

function fToC(fah){
	r = (fah - 32) * 5/9;
	return r;
}

function fToK(fah){
	r = (fah - 32) / 1.8000 + 273.15;
	return r;
}

function fToR(fah){
	r = fah + 459.67;
}

function cToF(cel){
	r = cel * (9/5) + 32;
}

function cToK(cel){
	r = cel + 273.15;
}

function cToR(cel){
	(cel + 273.15) * (9/5);
}

